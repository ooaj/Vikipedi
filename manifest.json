{
  "name": "VikipediPW",
  "short_name": "VikipediPW",
  "description": "Request all wikipedia.org requests to vikipedi.pw",
  "version": "0.0.5",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/VikipediPW.png"
  },
  "permissions":["*://*.wikipedia.org/*","webRequest","webRequestBlocking"]
}
